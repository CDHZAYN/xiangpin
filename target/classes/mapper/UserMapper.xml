<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tencent.wxcloudrun.dao.SeekerDao">

    <resultMap id="seekerPO" type="com.tencent.wxcloudrun.model.po.SeekerPO">
        <id column="seekerId" property="openID"/>
        <result column="seekerName" property="name"/>
        <result column="seekerPhone" property="phone"/>
        <result column="seekerOld" property="old"/>
        <result column="seekerEducation" property="education"/>
        <result column="seekerBirth" property="birth"/>
        <result column="seekerGander" property="gender"/>
    </resultMap>

    <select id="login" resultMap="seekerPO">
        select * from seekerTable;
    </select>

    <select id="getOpenID">
        select `seekerOpenId` from seekerTable where `seekerOpenId` = ${openID};
    </select>

</mapper>