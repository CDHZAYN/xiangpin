<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tencent.wxcloudrun.dao.LoginDao">
    <resultMap id="loginPo" type="com.tencent.wxcloudrun.model.po.LoginPO">
        <id column="openId" property="openId"/>
        <result column="userName" property="userName"/>
        <result column="userAvatar" property="userAvatar"/>
    </resultMap>

    <select id="login" resultMap="loginPo">
        select openId, userName, userAvatar from loginTable where openId=#{openId};
    </select>
    <select id="test" resultType="com.tencent.wxcloudrun.model.po.LoginPO">
        select * from loginTable;
    </select>

</mapper>